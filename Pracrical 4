// Находит простые числа 

#include <ostream>
using namespace std;
int main()
{
 int N, f, i, j;
 cout << "Insert the nuber, N:";
 cin >> N;
 for (i = 2;i <= N;i++)
  {
   f = 0;
   for (j = 2;j < i;j++)
    {
     if ((i % j == 0)) {f++;break;};
    }
   if ((f) == 0) cout << "The nuber is simple:" << i << endl;
  }
 system("pause");
}
